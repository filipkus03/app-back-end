<Window x:Class="app_back_end.rezerwacja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:app_back_end"
        mc:Ignorable="d"
        Title="rezerwacja" Height="900" Width="1000" Closed="Window_Closed">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="ID Części" />
            <TextBox Name="CzescIDTextBox" Margin="0,0,0,10"/>
            <Label Content="Ilość" />
            <TextBox Name="IloscTextBox" Margin="0,0,0,10"/>
            <Label Content="ID Zlecenia" />
            <TextBox Name="ZlecenieIDTextBox" Margin="0,0,0,10"/>
            <TextBlock Name="WynikTextBlock" Margin="10,0,0,0"/>
            <Button Content="Zarezerwuj" Click="Zarezerwuj_Click" Height="38"/>
        </StackPanel>
    </Grid>
</Window>
